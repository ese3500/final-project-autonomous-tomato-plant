#define F_CPU 16000000UL
#define BAUD_RATE 9600

#include <avr/interrupt.h>
#include <stdlib.h>
#include <stdio.h>
#include <avr/io.h>
#include <util/delay.h>

#include "uart.h"

void Initialize()
{
	// Pump output pin
	DDRD |= (1<<DDRD0);
}

int main(void)
{

	Initialize();

	while(1)
	{
		PORTD |= (1<<PORTD0);
		_delay_ms(100);
		PORTD &= ~(1<<PORTD0);
		_delay_ms(1000);
	}
}
